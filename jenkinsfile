pipeline {
    agent any
   
    stages {
        stage('this is call another job') {
            steps {
                sh 'echo this is 1st job'
               sh 'echo this is 2st job'
 sh 'echo this is 3st job'
            }
        }
        stage('this is for testing'){
            steps {
                sh 'echo testing'
              
            }
        }
        stage('this is for deploy') {
            steps {
               sh 'echo this is 1st jodeploy'
            }
        }
    }
}